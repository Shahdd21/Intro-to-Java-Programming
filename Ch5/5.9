import java.util.Scanner;

public class Demo
{
    public static void main(String[] args)
    {
        Scanner input = new Scanner(System.in);

        System.out.print("Enter the number of students : ");
        int t = input.nextInt();

        boolean firstIteration = true;

        int mx =-1, mx2 = -1;
        String st1 ="", st2="";

        while(t > 0)
        {
            System.out.print("Enter the name and the score of the student : ");
            String name = input.next();
            int score = input.nextInt();

            if(score > mx)
            {
                int temp = mx;
                mx = score;
                mx2 = temp;

                if(firstIteration)
                {
                    st1 = name;
                    firstIteration = false;
                }

                else
                {
                    String tempName = st1;
                    st1 = name;
                    st2 = tempName;
                }
            }

            else if (score > mx2)
            {
                mx2 = score;
                st2 = name;
            }

            --t;
        }

        System.out.printf("The name of the highest-score student is %s and their score is %d\n", st1,mx);
        System.out.printf("The name of the second-highest-score student is %s and their score is %d\n",st2,mx2);
    }
}
