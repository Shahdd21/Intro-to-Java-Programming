public class Demo
{
    public static void main(String[] args)
    {
       System.out.println("a\tb\tpow(a,b)");

       int a = 1 ;

       for(int i = 1 ; i <= 5 ; ++i)
       {
           int b = a+1 ;
           int power = (int)Math.pow(a,b) ;

           System.out.println(a + "\t" + b + "\t" + power);

           ++a;
       }
    }
}
